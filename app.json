      },
      "package": "com.diazbetancur.ECommerceapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": ["expo-localization"],
    "extra": {
      "eas": {
        "projectId": "7fa90564-c0e3-4a27-bfbc-1b379724b5c5"

{
  "cli": {
    "version": ">= 16.6.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true
    },
    "apk": {
      "env": {
        "ENVIRONMENT": "production"
      },
      "android": {
        "buildType": "apk"
      },
      "developmentClient": false,
      "distribution": "internal"
    },
    "ios-prod": {
      "env": {
        "ENVIRONMENT": "production"
      },
      "ios": {
        "simulator": false
      },
      "distribution": "internal",
      "developmentClient": false
    }
  },
  "submit": {
    "production": {}
  }
}
